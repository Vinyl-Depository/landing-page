{
    "auto_format_on_save": true,
    "auto_format_on_save_excludes": ["*.js"],
    "tabWidth": 4,
    "printWidth": 140,
    "useTabs": true,
    "singleQuote": true,
    "semi": true,
    "quoteProps": "consistent",
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "always",
    "jsxSingleQuote": false,
    "jsxBracketSameLine": false
}