{
	"name": "landing-page",
	"description": "Nextjs project for Vinyl Depository landing page",
	"version": "1.1.10",
	"private": true,
	"scripts": {
		"prepare": "husky install",
		"git:config": "git config --local include.path ../.gitconfig",
		"dev": "next dev -p 8080",
		"build": "next build",
		"postbuild": "next-sitemap --config next-sitemap.config.js",
		"start": "next start -p 80",
		"start:cypress": "next start -p 8080",
		"start:lhci": "next start -p 8080",
		"lint": "next lint && eslint -c cypress/.eslintrc.js cypress",
		"lint:fix": "next lint --fix && eslint -c cypress/.eslintrc.js cypress --fix",
		"format": "prettier --write \"**/*.{ts,tsx,scss,js,json}\"",
		"type-check": "tsc && tsc -p cypress/tsconfig.json",
		"stylelint": "stylelint \"src/**/*.scss\"",
		"stylelint:fix": "stylelint \"src/**/*.scss\" --fix",
		"lslint": "ls-lint",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:update-snaps": "jest --updateSnapshot",
		"cy:start": "run-s build start",
		"cy:run": "cypress run",
		"test:e2e": "start-server-and-test start:cypress / cy:run",
		"lighthouse": "lhci autorun",
		"depcheck": "depcheck",
		"template:page": "hygen component new-page",
		"template:container": "hygen component new-container",
		"template:layout": "hygen component new-layout",
		"template:ui": "hygen component new-ui"
	},
	"dependencies": {
		"axios": "0.26.1",
		"i18next": "21.6.14",
		"mailchimp-api-v3": "1.15.0",
		"next": "12.1.0",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-i18next": "11.16.2",
		"react-responsive-carousel": "3.2.23",
		"react-scroll": "1.8.6",
		"react-select": "5.2.2",
		"sharp": "0.30.3"
	},
	"devDependencies": {
		"@commitlint/cli": "16.2.3",
		"@commitlint/config-conventional": "16.2.1",
		"@lhci/cli": "0.9.0",
		"@ls-lint/ls-lint": "1.11.0",
		"@testing-library/jest-dom": "5.16.3",
		"@testing-library/react": "12.1.4",
		"@types/jest": "27.4.1",
		"@types/node": "17.0.14",
		"@types/react": "17.0.43",
		"@types/react-dom": "17.0.14",
		"@types/react-scroll": "1.8.3",
		"@typescript-eslint/eslint-plugin": "5.16.0",
		"@typescript-eslint/parser": "5.16.0",
		"commitizen": "4.2.4",
		"cypress": "9.5.2",
		"cz-format-extension": "1.5.0",
		"depcheck": "1.4.3",
		"eslint": "8.12.0",
		"eslint-config-next": "12.1.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-import": "2.25.4",
		"eslint-plugin-node": "11.1.0",
		"eslint-plugin-react-hooks": "4.3.0",
		"eslint-plugin-unused-imports": "2.0.0",
		"husky": "7.0.4",
		"hygen": "6.2.0",
		"jest": "27.5.1",
		"lint-staged": "12.3.7",
		"next-sitemap": "2.5.14",
		"prettier": "2.6.1",
		"sass": "1.49.9",
		"start-server-and-test": "1.14.0",
		"word-wrap": "1.2.3",
		"stylelint": "14.6.1",
		"stylelint-config-prettier-scss": "0.0.1",
		"stylelint-config-recess-order": "3.0.0",
		"stylelint-config-standard-scss": "3.0.0",
		"typescript": "4.6.3"
	}
}
