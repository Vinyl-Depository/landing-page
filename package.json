{
	"name": "landing-page",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"prepare": "husky install",
		"dev": "next dev -p 80",
		"build": "next build",
		"start": "next start -p 80",
		"lint": "next lint",
		"format": "prettier --write \"**/*.{ts,tsx,scss,js,json}\"",
		"type-check": "tsc --noEmit",
		"stylelint": "stylelint src/**/*.scss --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:update-snaps": "jest --updateSnapshot",
		"template:page": "hygen component new-page",
		"template:container": "hygen component new-container",
		"template:layout": "hygen component new-layout",
		"template:ui": "hygen component new-ui"
	},
	"config": {
		"commitizen": {
			"path": "git-cz"
		}
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"dependencies": {
		"i18next": "^21.6.10",
		"mailchimp-api-v3": "^1.15.0",
		"next": "12.0.10",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-i18next": "^11.15.3",
		"react-responsive-carousel": "^3.2.22"
	},
	"devDependencies": {
		"@commitlint/cli": "^16.1.0",
		"@commitlint/config-conventional": "^16.0.0",
		"@testing-library/jest-dom": "^5.16.2",
		"@testing-library/react": "^12.1.2",
		"@types/jest": "^27.4.0",
		"@types/node": "17.0.14",
		"@types/react": "17.0.39",
		"@types/react-dom": "^17.0.11",
		"@typescript-eslint/eslint-plugin": "^5.10.2",
		"@typescript-eslint/parser": "^5.10.2",
		"commitizen": "^4.2.4",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "8.8.0",
		"eslint-config-next": "12.0.10",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-react": "^7.28.0",
		"eslint-plugin-react-hooks": "^4.3.0",
		"eslint-plugin-unused-imports": "^2.0.0",
		"git-cz": "^4.8.0",
		"husky": "^7.0.4",
		"hygen": "^6.1.0",
		"jest": "^27.4.7",
		"prettier": "^2.5.1",
		"sass": "^1.49.7",
		"stylelint": "^14.5.0",
		"stylelint-config-prettier-scss": "^0.0.1",
		"stylelint-config-recess-order": "^3.0.0",
		"stylelint-config-standard-scss": "^3.0.0",
		"stylelint-order": "^5.0.0",
		"stylelint-scss": "^4.1.0",
		"typescript": "4.5.5"
	}
}
