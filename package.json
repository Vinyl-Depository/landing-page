{
	"name": "landing-page-front",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@testing-library/jest-dom": "^5.15.1",
		"@testing-library/react": "^11.2.7",
		"@testing-library/user-event": "^12.8.3",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.6",
		"i18next": "^21.5.4",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-i18next": "^11.14.3",
		"react-responsive-carousel": "^3.2.22",
		"react-scripts": "4.0.3",
		"web-vitals": "^1.1.2"
	},
	"scripts": {
		"prepare": "husky install",
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"test:no-watch": "react-scripts test --watchAll=false",
		"test:cov": "react-scripts test --watchAll=false --coverage",
		"lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
		"format": "prettier --write \"src/**/*.{ts,tsx}\"",
		"template:page": "hygen component new-page",
		"template:container": "hygen component new-container",
		"template:layout": "hygen component new-layout",
		"template:ui": "hygen component new-ui",
		"eject": "react-scripts eject"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@commitlint/cli": "^16.1.0",
		"@commitlint/config-conventional": "^16.0.0",
		"@types/enzyme-adapter-react-16": "^1.0.6",
		"@types/jest": "^27.0.3",
		"@types/node": "^16.11.11",
		"@types/react": "^17.0.37",
		"@types/react-dom": "^17.0.11",
		"@typescript-eslint/eslint-plugin": "^5.5.0",
		"@typescript-eslint/parser": "^5.5.0",
		"commitizen": "^4.2.4",
		"cz-conventional-changelog": "^3.3.0",
		"eslint-plugin-react": "^7.27.1",
		"eslint-plugin-unused-imports": "^2.0.0",
		"git-cz": "^4.8.0",
		"husky": "^7.0.4",
		"hygen": "^6.1.0",
		"node-sass": "^6.0.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.5.0",
		"typescript": "^4.5.2"
	},
	"config": {
		"commitizen": {
			"path": "git-cz"
		}
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	}
}
