{
	"name": "landing-page-front",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@testing-library/jest-dom": "^5.15.1",
		"@testing-library/react": "^11.2.7",
		"@testing-library/user-event": "^12.8.3",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.6",
		"i18next": "^21.5.4",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-i18next": "^11.14.3",
		"react-responsive-carousel": "^3.2.22",
		"react-scripts": "4.0.3",
		"web-vitals": "^1.1.2"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"test:no-watch": "react-scripts test --watchAll=false --coverage",
		"eject": "react-scripts eject",
		"lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
		"format": "prettier --write \"src/**/*.{ts,tsx}\"",
		"template:page": "hygen component new-page",
		"template:container": "hygen component new-container",
		"template:layout": "hygen component new-layout",
		"template:ui": "hygen component new-ui"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@types/enzyme-adapter-react-16": "^1.0.6",
		"@types/jest": "^27.0.3",
		"@types/node": "^16.11.11",
		"@types/react": "^17.0.37",
		"@types/react-dom": "^17.0.11",
		"@typescript-eslint/eslint-plugin": "^5.5.0",
		"@typescript-eslint/parser": "^5.5.0",
		"eslint-plugin-react": "^7.27.1",
		"husky": "^7.0.4",
		"hygen": "^6.1.0",
		"node-sass": "^6.0.1",
		"prettier": "^2.5.0",
		"typescript": "^4.5.2"
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{ts,tsx}",
			"!src/**/*.d.ts"
		],
		"coveragePathIgnorePatterns": [
			"<rootDir>/src/index.tsx",
			"<rootDir>/src/index.tsx",
			"<rootDir>/src/setupTests.ts",
			"<rootDir>/src/reportWebVitals.ts",
			"<rootDir>/src/assets/",
			"<rootDir>/src/i18n/"
		],
		"coverageThreshold": {
			"global": {
				"branches": 30,
				"functions": 30,
				"lines": 30,
				"statements": 30
			}
		}
	}
}
