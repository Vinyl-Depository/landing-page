{
	"name": "landing-page",
	"description": "Nextjs project for Vinyl Depository landing page",
	"version": "1.1.6",
	"private": true,
	"scripts": {
		"prepare": "husky install",
		"git:config": "git config --local include.path ../.gitconfig",
		"dev": "next dev -p 80",
		"build": "next build",
		"postbuild": "next-sitemap --config next-sitemap.config.js",
		"start": "next start -p 80",
		"lint": "next lint",
		"lint:fix": "next lint --fix",
		"format": "prettier --write \"**/*.{ts,tsx,scss,js,json}\"",
		"type-check": "tsc --noEmit",
		"stylelint": "stylelint \"src/**/*.scss\"",
		"stylelint:fix": "stylelint \"src/**/*.scss\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:update-snaps": "jest --updateSnapshot",
		"lighthouse": "lhci autorun",
		"dep-check": "depcheck",
		"template:page": "hygen component new-page",
		"template:container": "hygen component new-container",
		"template:layout": "hygen component new-layout",
		"template:ui": "hygen component new-ui"
	},
	"dependencies": {
		"axios": "0.26.1",
		"i18next": "21.6.14",
		"mailchimp-api-v3": "1.15.0",
		"next": "12.1.0",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-ga": "3.3.0",
		"react-gtm-module": "2.0.11",
		"react-i18next": "11.15.6",
		"react-responsive-carousel": "3.2.23",
		"react-scroll": "1.8.6",
		"react-select": "5.2.2",
		"sharp": "0.30.2"
	},
	"devDependencies": {
		"@commitlint/cli": "16.2.1",
		"@commitlint/config-conventional": "16.2.1",
		"@lhci/cli": "0.9.0",
		"@testing-library/jest-dom": "5.16.2",
		"@testing-library/react": "12.1.4",
		"@types/jest": "27.4.1",
		"@types/node": "17.0.14",
		"@types/react": "17.0.40",
		"@types/react-dom": "17.0.13",
		"@types/react-gtm-module": "2.0.1",
		"@types/react-scroll": "1.8.3",
		"@typescript-eslint/eslint-plugin": "5.14.0",
		"@typescript-eslint/parser": "5.14.0",
		"commitizen": "4.2.4",
		"cz-conventional-changelog": "3.3.0",
		"depcheck": "^1.4.3",
		"eslint": "8.10.0",
		"eslint-config-next": "12.1.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-import": "2.25.4",
		"eslint-plugin-node": "11.1.0",
		"eslint-plugin-react": "7.29.3",
		"eslint-plugin-react-hooks": "4.3.0",
		"eslint-plugin-unused-imports": "2.0.0",
		"git-cz": "4.8.0",
		"husky": "7.0.4",
		"hygen": "6.2.0",
		"jest": "27.5.1",
		"lint-staged": "12.3.5",
		"next-sitemap": "2.5.7",
		"prettier": "2.5.1",
		"sass": "1.49.9",
		"stylelint": "14.5.3",
		"stylelint-config-prettier-scss": "0.0.1",
		"stylelint-config-recess-order": "3.0.0",
		"stylelint-config-standard-scss": "3.0.0",
		"typescript": "4.6.2"
	}
}
