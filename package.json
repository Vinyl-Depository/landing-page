{
	"name": "landing-page",
	"description": "Nextjs project for Vinyl Depository landing page",
	"version": "1.4.0",
	"private": true,
	"scripts": {
		"preinstall": "is-ci || npx only-allow yarn",
		"husky:install": "husky install",
		"git:config": "git config --local include.path ../.gitconfig",
		"prepare": "is-ci || run-p husky:install git:config",
		"dev": "next dev -p 8080",
		"build": "next build",
		"postbuild": "next-sitemap --config next-sitemap.config.js",
		"start": "next start -p 80",
		"start:cypress": "cross-env NODE_ENV=test next start -p 8080",
		"start:lhci": "next start -p 8080",
		"lint": "next lint && eslint -c cypress/.eslintrc.js cypress",
		"lint:fix": "next lint --fix && eslint -c cypress/.eslintrc.js cypress --fix",
		"format": "prettier --write \"**/*.{ts,tsx,scss,js,json}\"",
		"type-check": "tsc && tsc -p cypress/tsconfig.json",
		"stylelint": "stylelint \"src/**/*.scss\"",
		"stylelint:fix": "stylelint \"src/**/*.scss\" --fix",
		"inflint": "inflint -c inflint.config.ts",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:update-snaps": "jest --updateSnapshot",
		"cy:start": "run-s build start",
		"cy:run": "cypress run",
		"test:e2e": "start-server-and-test start:cypress / cy:run",
		"lighthouse": "lhci autorun",
		"depcheck": "depcheck --config .depcheckrc.json",
		"template:page": "hygen component new-page",
		"template:container": "hygen component new-container",
		"template:layout": "hygen component new-layout",
		"template:ui": "hygen component new-ui"
	},
	"dependencies": {
		"@builder.io/partytown": "0.7.3",
		"axios": "0.27.2",
		"i18next": "21.10.0",
		"next": "12.3.4",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-i18next": "11.18.6",
		"react-responsive-carousel": "3.2.23",
		"react-scroll": "1.8.8",
		"react-select": "5.7.0",
		"sharp": "0.31.2"
	},
	"devDependencies": {
		"@commitlint/cli": "16.3.0",
		"@lhci/cli": "0.10.0",
		"@testing-library/jest-dom": "5.16.5",
		"@testing-library/react": "13.4.0",
		"@types/jest": "27.5.2",
		"@types/node": "17.0.14",
		"@types/react": "17.0.52",
		"@types/react-dom": "18.0.9",
		"@types/react-scroll": "1.8.5",
		"@typescript-eslint/eslint-plugin": "5.46.0",
		"@typescript-eslint/parser": "5.46.0",
		"commitizen": "4.2.6",
		"cross-env": "7.0.3",
		"cypress": "9.7.0",
		"cz-vinyl": "1.6.2",
		"depcheck": "1.4.3",
		"eslint": "8.29.0",
		"eslint-config-next": "12.3.4",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-node": "11.1.0",
		"eslint-plugin-unused-imports": "2.0.0",
		"husky": "7.0.4",
		"hygen": "6.2.11",
		"inflint": "1.2.2",
		"is-ci": "3.0.1",
		"jest": "27.5.1",
		"lint-staged": "12.5.0",
		"next-sitemap": "2.5.28",
		"npm-run-all": "4.1.5",
		"prettier": "2.8.1",
		"sass": "1.56.2",
		"start-server-and-test": "1.15.2",
		"stylelint": "14.16.0",
		"stylelint-config-prettier-scss": "0.0.1",
		"stylelint-config-recess-order": "3.0.0",
		"stylelint-config-standard-scss": "3.0.0",
		"typescript": "4.9.4"
	},
	"resolutions": {
		"minimist": "1.2.7"
	}
}
